<%= render "shared/header" %>
<div class="skill_header">ログイン</div>

<div class="skill_new">
  <div class="skill_new-form">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

      <div class="form-label"><%= f.label :Emailアドレス %></div>
        <%= f.email_field :email, autofocus: true, autocomplete: "email", class: 'form-box' %>
      

      <div class="form-label"><%= f.label :パスワード %></div>
        <%= f.password_field :password, autocomplete: "current-password", class: 'form-box' %>

      <% if devise_mapping.rememberable? %>
        <div class="form-label">
          <%= f.check_box :remember_me %>
          <%= f.label :パスワードを保存する %>
        </div>
      <% end %>

      <div class="actions">
        <%= f.submit "ログイン", class: 'form-submit' %>
      </div>
    <% end %>
  </div>
  <br><%= render "devise/shared/links" %>
</div>
